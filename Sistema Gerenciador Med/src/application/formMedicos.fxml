<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FormMedicosController">
   <children>
      <Label layoutX="210.0" prefHeight="64.0" prefWidth="208.0" text="Novo Médico">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="70.0" text="nome: " />
      <TextField fx:id="name" layoutX="78.0" layoutY="66.0" />
      <Label layoutX="266.0" layoutY="70.0" text="sobrenome:" />
      <TextField fx:id="sobreName" layoutX="344.0" layoutY="66.0" />
      <Label layoutX="41.0" layoutY="108.0" text="CPF:" />
      <TextField fx:id="cpf" layoutX="78.0" layoutY="104.0" />
      <Label layoutX="266.0" layoutY="100.0" text="RG:" />
      <TextField fx:id="rg" layoutX="298.0" layoutY="96.0" />
  
      <Label layoutX="27.0" layoutY="204.0" text="nome do longa:" />
      <TextField fx:id="longa" layoutX="137.0" layoutY="200.0" />
      <Label layoutX="277.0" layoutY="166.0" text="Endereço" />
      <Label layoutX="62.0" layoutY="240.0" text="número:" />
      <TextField fx:id="num" layoutX="118.0" layoutY="236.0" prefHeight="24.0" prefWidth="46.0" />
      <Label layoutX="175.0" layoutY="240.0" text="complemento:" />
      <TextField fx:id="comple" layoutX="278.0" layoutY="240.0" />
      <Label layoutX="469.0" layoutY="252.0" text="Bairro:" />
      <TextField fx:id="bairro" layoutX="515.0" layoutY="248.0" prefHeight="24.0" prefWidth="143.0" />
      <Label layoutX="17.0" layoutY="292.0" text="cidade:" />
      <TextField fx:id="city" layoutX="78.0" layoutY="288.0" />
      <Label layoutX="266.0" layoutY="292.0" text="estado:" />
      <ComboBox fx:id="esta" layoutX="321.0" layoutY="285.0" onAction="#mostrar" prefHeight="31.0" prefWidth="113.0" promptText="selecione" />
      <Label layoutX="461.0" layoutY="293.0" text="CEP:" />
      <TextField fx:id="cep" layoutX="505.0" layoutY="289.0" />
      <Button layoutX="714.0" layoutY="524.0" mnemonicParsing="false" onAction="#save">
         <graphic>
            <ImageView fitHeight="54.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/savedisk_121993.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="10.0" layoutY="14.0" mnemonicParsing="false" onAction="#voltarLista">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/back_arrow_5821.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="38.0" layoutY="396.0" text="CRM:" />
      <TextField fx:id="crm" layoutX="89.0" layoutY="392.0" />
      <Label layoutX="476.0" layoutY="100.0" text="Telefone:" />
      <TextField fx:id="tel" layoutX="543.0" layoutY="96.0" />
      <ComboBox fx:id="esp" layoutX="289.0" layoutY="392.0" prefWidth="150.0" promptText="Especialidade" />
      <ComboBox fx:id="subEsp" layoutX="462.0" layoutY="392.0" prefHeight="24.0" prefWidth="162.0" promptText="Subespecialidade" />
   </children>
</AnchorPane>
